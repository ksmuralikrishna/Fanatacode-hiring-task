{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Jwt": {
        "Key": "ThisIsASecretKeyForJwtTokenDontShare"
    },
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "GeneralRules": [
            {
                "Endpoint": "POST:/api/auth/login",
                "Period": "1m",
                "Limit": 5
            }
        ]
    },
    "ConnectionStrings": {
        "DefaultConnection": "server=localhost;port=3306;database=world;user=root;password=mysql123@"
    },
    "AllowedHosts": "*"
}
